<amanzi_input version="2.0.0" type="unstructured">

	<model_description name="linear_head_head_1d">
		<comments>Steady-State Saturated: prescribed flux and head BCs, linear pressure profile</comments>
		<model_id>1SSConFlow2</model_id>
		<author>Greg Flach</author>
		<units>
			<length_unit>m</length_unit>
			<time_unit>s</time_unit>
			<mass_unit>kg</mass_unit>
			<conc_unit>molar</conc_unit>
		</units>
	</model_description>

	<definitions>
		<macros>
			<time_macro name="Steady State">
				<time>0.0</time>
			</time_macro>
		</macros>	
	</definitions>

	<process_kernels>
		<comments>Single phase flow only</comments>
      		<flow state = "on" model = "saturated"/>
      		<transport state = "off" algorithm="none"/>
      		<chemistry state = "off" process_model="none"/>
	</process_kernels>

	<phases>
		<liquid_phase name="water">
			<eos>false</eos>
			<viscosity>1.002E-03</viscosity>
			<density>998.2</density>
		</liquid_phase>
	</phases>

	<execution_controls>
		<verbosity level="high"/>
		<execution_control_defaults mode="steady" method="bdf1" />
		<execution_control  start="0.0" end="0.0" init_dt="1000" mode = "steady" method="bdf1" />
	</execution_controls>

	<numerical_controls>
	        <linear_solver>
			<max_iterations>10</max_iterations>
			<tolerance>1.0e-18</tolerance>
		</linear_solver>
	</numerical_controls>

        <mesh framework="mstk">
		<comments>Three-dimensional box 100m x 2m x 10m</comments>
		<dimension>3</dimension>
		<generate>
			<number_of_cells nx = "20"  ny = "1"  nz = "1"/>
			<box  low_coordinates = "0.0, 0.0, 0.0" high_coordinates = "100.0, 2.0, 10.0"/>
		</generate>
	</mesh>

	<regions>
  		<comments></comments>
		<region name ="Entire Domain">
			<comments>One region comprising the entire domain</comments>
			<box low_coordinates= "0.0, 0.0, 0.0"     high_coordinates="100.0, 2.0, 10.0"/>
		</region>
		<region name ="Left">
			<comments>Left boundary</comments>
			<box low_coordinates= "0.0, 0.0, 0.0"     high_coordinates="0.0, 2.0, 10.0"/>
		</region>
		<region name ="Right">
			<comments>Right boundary</comments>
			<box low_coordinates= "100.0, 0.0, 0.0"     high_coordinates="100.0, 2.0, 10.0"/>
		</region>
		<point name="LeftBoundary"      coordinate="(  0.0, 1.0, 5.0)"/>
		<point name="RightBoundary"     coordinate="(100.0, 1.0, 5.0)"/>
		<point name="Midpoint"          coordinate="( 50.0, 1.0, 5.0)"/>
		<point name="LeftmostCell"      coordinate="(  2.5, 1.0, 5.0)"/>
		<point name="RightmostCell"     coordinate="( 97.5, 1.0, 5.0)"/>
		<point name="LeftMidpointCell"  coordinate="( 47.5, 1.0, 5.0)"/>
		<point name="RightMidpointCell" coordinate="( 52.5, 1.0, 5.0)"/>
	</regions>
	
	<materials>
		<material name="Soil">
		       	<comments>Homogeneous material properties throughout the entire domain</comments>
               		<mechanical_properties>
                         	<porosity value="0.25"/>
 	 	      	</mechanical_properties>
 	 	        <permeability x="1.1847e-12" y="1.1847e-12" z="1.1847e-12"/>
 	 	      	<assigned_regions>Entire Domain</assigned_regions>
 	   	</material>
	</materials>

	<initial_conditions>
		<initial_condition name="Initial Condition">
			<comments>Uniform pressure over entire domain</comments>
			<assigned_regions>Entire Domain</assigned_regions>
			<liquid_phase name = "water">
				<liquid_component name = "water">
					<pressure value = "101325.0" function="uniform"/>
				</liquid_component>
			</liquid_phase>
         	</initial_condition>
	</initial_conditions>

	<boundary_conditions>
		<comments></comments>
		<boundary_condition name = "LeftBC">
			<comments>Boundary condition at x=0</comments>
			<assigned_regions>Left</assigned_regions>
			<liquid_phase name = "water">
				<liquid_component name = "water">
					<inward_mass_flux start="0.0" function= "constant" value="1.1553e-4"/>
				</liquid_component>
			</liquid_phase>
		</boundary_condition>

		<boundary_condition name = "RightBC">
			<comments>Boundary condition at x=L</comments>
			<assigned_regions>Right</assigned_regions>
			<liquid_phase name = "water">
				<liquid_component name = "water">
					<hydrostatic start="0.0" function= "constant" value="19.0"/>
				</liquid_component>
			</liquid_phase>
		</boundary_condition>
	</boundary_conditions>

	<output>

	        <observations>
		        <filename>observation.out</filename>
			<liquid_phase name="water">
			        <aqueous_pressure>
				        <assigned_regions>LeftBoundary</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>RightBoundary</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>Midpoint</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>LeftmostCell</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>RightmostCell</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>LeftMidpointCell</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			        <aqueous_pressure>
				        <assigned_regions>RightMidpointCell</assigned_regions>
				        <functional>point</functional>
				        <time_macro>Steady State</time_macro>
				</aqueous_pressure>
			</liquid_phase>
		</observations>

		<vis>
			<base_filename>steady-flow</base_filename>
		  	<num_digits>5</num_digits>
		  	<time_macro>Steady State</time_macro>
		</vis>
		
	</output>

</amanzi_input>

