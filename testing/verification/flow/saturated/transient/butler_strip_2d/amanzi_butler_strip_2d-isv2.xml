<amanzi_input version="2.0.0" type="unstructured">

  <model_description name="SR Test Problem">

    <comments>Confind aquifer transient drawdown (Theis 1935)</comments>
    <model_id>Theis</model_id>
    <author>Dylan Harp, Alec Thomas, David Moulton</author>
    <units>
      <length_unit>m</length_unit>
      <time_unit>s</time_unit>
      <mass_unit>kg</mass_unit>
      <conc_unit>molar</conc_unit>
    </units>

  </model_description>
  
  <!--

    Macro Definitions

  -->
  <definitions>

    <macros>
      <time_macro name="Observation Times">
	<time>0.0</time>
        <time>1</time>
        <time>3</time>
        <time>7</time>
        <time>15</time>
        <time>31</time>
        <time>63</time>
        <time>127</time>
        <time>255</time>
        <time>511</time>
        <time>1023</time>
        <time>2047</time>
        <time>4095</time>
        <time>8191</time>
        <time>16383</time>
        <time>32677</time>
        <time>65535</time>
        <time>131071</time>
        <time>262143</time>
        <time>524287</time>
        <time>1e6</time>
        <time>1e7</time>
        <time>1e8</time>
        <time>1e9</time>
      </time_macro>
    </macros>	

  </definitions>

  <!--

    Process Kernels

  -->
  <process_kernels>
    <comments>Single-phase transient flow</comments>
    <flow state = "on" model = "saturated"/>
    <transport state = "off" algorithm = "none"/>
    <chemistry state = "off" process_model="none"/>
  </process_kernels>
  

  <!--

    Execution Control

  -->
  <execution_controls>
    <verbosity level="high"/>
    <execution_control_defaults init_dt= "3.64" increase_factor="1.5" method="bdf1"/>
    <execution_control start="0.0" end="1.64e7" init_dt="3.64" mode="transient"/>
  </execution_controls>

  <!--

    Numerical Control Parameters

  -->
  <numerical_controls>
    <linear_solver>
      <max_iterations>1000</max_iterations>
      <tolerance>1.0e-16</tolerance>
    </linear_solver>
  </numerical_controls>
  
  <!--

    Mesh

  -->
  <mesh framework="mstk">
    <comments>Three-dimensional box 100m x 50m x 50m</comments>
    <dimension>3</dimension>
    <generate>
      <number_of_cells nx="601"  ny="601"  nz="1"/>
      <box  low_coordinates="-1202.0,-1202.0,0.0"  high_coordinates="1202.0,1202.0,1.0"/>
    </generate>
  </mesh>

  <!--

    Output

  -->
  <regions>

    <region name ="Entire Domain">
      <comments>One region covering the entire domain</comments>
      <box  low_coordinates="-1202.0,-1202.0,0.0"  high_coordinates="1202.0,1202.0,1.0"/>
    </region>

    <region name ="Zone_1">
      <comments>The left zone</comments>
      <box  low_coordinates="-1202.0,-1202.0,0.0"  high_coordinates="-8.0,1202.0,1.0"/>
    </region>

    <region name ="Zone_2">
      <comments>The strip</comments>
      <box  low_coordinates="-8.0,-1202.0,0.0"  high_coordinates="8.0,1202.0,1.0"/>
    </region>

    <region name ="Zone_3">
      <comments>The right zone</comments>
      <box  low_coordinates="8.0,-1202.0,0.0"  high_coordinates="1202.0,1202.0,1.0"/>
    </region>

    <region name ="Well">
     <comments>Pumping Well</comments>
      <box low_coordinates= "-1.0,-1.0,0.0" high_coordinates="1.0,1.0,1.0"/>
    </region>

    <region name ="yz1">
     <comments>Left Plane</comments>
      <plane location="-1202.0,-1202.0,0.0" normal="-1.0,0.0,0.0"/>
    </region>
    <!--
    <plane name="yz1" location="-1202.0,-1202.0,0.0" normal="-1.0,0.0,0.0"/>
    -->
    <plane name="yz2" location="1202.0,-1202.0,0.0" normal="1.0,0.0,0.0"/>
    <plane name="xz1" location="-1202.0,-1202.0,0.0" normal="0.0,-1.0,0.0"/>
    <plane name="xz2" location="-1202.0,1202.0,0.0" normal="0.0,1.0,0.0"/>

    <point name="Obs_r24" coordinate="(24.0,0.0,1.0)"/>
    <point name="Obs_r100" coordinate="(100.0,0.0,1.0)"/>

  </regions>
  

  <!--

    Phases

  -->
  <phases>
    <liquid_phase name="water">
      <eos>false</eos>
      <viscosity>1.002E-03</viscosity>
      <density>1000.0</density>
    </liquid_phase>
  </phases>

  <!--

    Materials

  -->
  <materials>

    <material name="Soil_1">
      <comments>Material properties for zone 1</comments>
      <assigned_regions>Zone_1</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25"/>
	<specific_storage value="5.0e-4"/>
      </mechanical_properties>
      <permeability x="1.187E-8" y="1.187E-8" z="1.187E-8"/>
    </material>

    <material name="Soil_2">
      <comments>Material properties for zone 2</comments>
      <assigned_regions>Zone_2</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25"/>
	<specific_storage value="2.0e-4"/>
      </mechanical_properties>
      <permeability x="1.187E-9" y="1.187E-9" z="1.187E-9"/>
    </material>

    <material name="Soil_3">
      <comments>Material properties for zone 3</comments>
      <assigned_regions>Zone_3</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25"/>
	<specific_storage value="2.0e-5"/>
      </mechanical_properties>
      <permeability x="1.187E-10" y="1.187E-10" z="1.187E-10"/>
    </material>

  </materials>

  <!--

    Initial Conditions

  -->
  <initial_conditions>

    <initial_condition name="Initial Condition">
      <comments>Constant pressure of 100m</comments>
      <assigned_regions>Entire Domain</assigned_regions>
      <liquid_phase name = "water">
	<liquid_component name = "water">
	  <linear_pressure value = "1.07785e6" reference_coord ="(1202.0,1202.0,10.0)" gradient="(0.0,0.0,0)"/>
	</liquid_component>
      </liquid_phase>
    </initial_condition>

  </initial_conditions>
	

  <!--

    Boundary Conditions

  -->
  <boundary_conditions>

    <boundary_condition name = "Far Field Head">
      <comments>Far field static head of the confined aquifer</comments>
      <assigned_regions>xz1,xz2,yz1,yz2</assigned_regions>
      <liquid_phase name = "water">
	<liquid_component name = "water">
	  <uniform_pressure start="1.07785e6" function= "constant" value="1.07785e6"/>
	</liquid_component>
      </liquid_phase>
    </boundary_condition>

  </boundary_conditions>

  <sources>
    <source name = "Pumping Well" >
      <assigned_regions>Well</assigned_regions>
      <liquid_phase name = "water">
	<liquid_component name="water">
	  <volume_weighted start="0.0" function="constant" value="-11.5485"/>
	</liquid_component>
      </liquid_phase>
    </source>
  </sources>
  <!--

    Output

  -->
  <output>
    
    <observations>

      <filename>observations.out</filename>

      <liquid_phase name="water">
	<aqueous_pressure>
	  <assigned_regions>Obs_r24</assigned_regions>
	  <functional>point</functional>
	  <time_macro>Observation Times</time_macro>
	</aqueous_pressure>
	<aqueous_pressure>
	  <assigned_regions>Obs_r100</assigned_regions>
	  <functional>point</functional>
	  <time_macro>Observation Times</time_macro>
	</aqueous_pressure>
      </liquid_phase>


    </observations>
    
  </output>

</amanzi_input>

