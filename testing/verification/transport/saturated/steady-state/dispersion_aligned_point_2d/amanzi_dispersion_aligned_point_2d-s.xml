<ParameterList name="Main">
  <Parameter name="Amanzi Input Format Version" type="string" value="1.2.0"/>

  <ParameterList name="Execution Control">
    <Parameter name="Flow Model" type="string" value="Off"/>
    <Parameter name="Transport Model" type="string" value="On"/>
    <Parameter name="Chemistry Model" type="string" value="Off"/>

    <ParameterList name="Time Integration Mode">
      <ParameterList name="Transient">
        <Parameter name="Start" type="double" value="0.0"/>
        <Parameter name="End" type="double" value="1.2096e+8"/>
        <Parameter name="Maximum Cycle Number" type="int" value="50000"/>
        <Parameter name="Maximum Time Step Grow" type="double" value="1"/>
        <Parameter name="Initial Time Step Multiplier" type="double" value="1"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Numerical Control Parameters">
      <ParameterList name="Basic Algorithm Control">
        <ParameterList name="Expert Settings">
	  <Parameter name="cfl" type="double" value=".0306662"/>
	  <Parameter name="cfl" type="double" value="1"/>
	  <Parameter name="cfl" type="double" value="0.7"/>
	  <Parameter name="visc_tol" type="double" value="1.e-12"/>
	  <Parameter name="be_cn_theta_trac" type="double" value="0.5"/>
	</ParameterList>
      </ParameterList>
      <ParameterList name="Iterative Linear Solver Control">
	<ParameterList name="Multigrid Algorithm">
	  <ParameterList name="Expert Settings">
	    <Parameter name="usecg" type="int" value="0"/>
	    <Parameter name="v" type="int" value="3"/>
	    <Parameter name="maxiter" type="int" value="100"/>
	    <Parameter name="numLevelsMAX" type="int" value="200"/>
	  </ParameterList>
	</ParameterList>
      </ParameterList>

      <ParameterList name="Adaptive Mesh Refinement Control">
        <Parameter name="Blocking Factor" type="Array(int)" value="{2}"/>
        <Parameter name="Maximum Grid Size" type="Array(int)" value="{160}"/>
        <Parameter name="Maximum Grid Size" type="Array(int)" value="{64}"/>
      </ParameterList>

    </ParameterList>

    <Parameter name="Verbosity" type="string" value="High"/>
  </ParameterList>
      
  <ParameterList name="Domain">
    <Parameter name="Spatial Dimension" type="int" value="2"/>
  </ParameterList>

  <ParameterList name="Mesh">
    <ParameterList name="Structured">
      <!-- <Parameter name="Domain Low Corner"  type="Array(double)" value="{-270,-270}"/> -->
      <!-- <Parameter name="Domain High Corner" type="Array(double)" value="{ 930, 270}"/> -->
      <!-- <Parameter name="Number of Cells"    type="Array(int)"    value="{  80, 36}"/> -->
      <Parameter name="Domain Low Corner"  type="Array(double)" value="{-270,-300}"/>
      <Parameter name="Domain High Corner" type="Array(double)" value="{ 930, 300}"/>
      <Parameter name="Number of Cells"    type="Array(int)"    value="{ 256, 128}"/>
      <Parameter name="Number of Cells"    type="Array(int)"    value="{ 512, 256}"/>
      <Parameter name="Number of Cells"    type="Array(int)"    value="{ 128, 64}"/>
    </ParameterList>
  </ParameterList>

