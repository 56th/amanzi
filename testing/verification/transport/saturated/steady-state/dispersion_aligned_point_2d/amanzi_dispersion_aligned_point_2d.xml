<ParameterList name="Main">
  <Parameter name="Amanzi Input Format Version" type="string" value="1.2.0"/>

  <ParameterList name="Execution Control">
    <Parameter name="Flow Model" type="string" value="Off"/>
    <Parameter name="Transport Model" type="string" value="On"/>
    <Parameter name="Chemistry Model" type="string" value="Off"/>

    <ParameterList name="Time Integration Mode">
      <ParameterList name="Transient">
        <Parameter name="Start" type="double" value="0.0"/>
        <Parameter name="End" type="double" value="1.2096e+8"/>
        <Parameter name="Initial Time Step" type="double" value="1.0"/>
      </ParameterList>
    </ParameterList>

    <Parameter name="Verbosity" type="string" value="High"/>

    <ParameterList name="Numerical Control Parameters">
      <ParameterList name="Unstructured Algorithm">
        <Parameter name="linear solver tolerance" type="double" value="1e-20"/>
        <Parameter name="linear solver maximum iterations" type="int" value="100"/> 
        <ParameterList name="Transport Process Kernel">
          <Parameter name="transport subcycling" type="bool" value="false"/>
          <Parameter name="Transport Integration Algorithm" type="string" value="Explicit First-Order"/>
          <Parameter name="CFL" type="double" value="1.0"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>
      
  <!-- MESH -->
  <ParameterList name="Domain">
    <Parameter name="Spatial Dimension" type="int" value="2"/>
  </ParameterList>

  <ParameterList name="Mesh">
    <ParameterList name="Unstructured">
      <ParameterList name="Expert">
        <Parameter name="Framework" type="string" value="MSTK"/>
        <Parameter name="Verify Mesh" type="bool" value="false"/>
      </ParameterList>
      <ParameterList name="Generate Mesh">
        <ParameterList name="Uniform Structured">
          <Parameter name="Domain Low Corner" type="Array(double)" value="{-271.0, -270.0}"/>
          <Parameter name="Domain High Corner" type="Array(double)" value="{960.0,  270.0}"/>
          <Parameter name="Number of Cells" type="Array(int)" value="{84, 37}"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!-- REGIONS -->
  <ParameterList name="Regions">
    <ParameterList name="All domain">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{-271.0, -270.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{960.0,  270.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Well">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{-5.0,-5.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{5.0, 5.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Dnwind150">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{-150.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Dnwind090">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{-90.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Dnwind030">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{-30.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind030">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{30.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind090">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{90.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind150">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{150.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind210">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{210.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind270">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{270.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind330">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{330.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind390">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{390.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Upwind450">
      <ParameterList name="Region: Point">
        <Parameter name="Coordinate" type="Array(double)" value="{450.0,0.0}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!-- Materials -->
  <ParameterList name="Material Properties">
    <ParameterList name="Aquifers">
      <Parameter name="Assigned Regions" type="Array(string)" value="{All domain}"/>
      <ParameterList name="Porosity: Uniform">
        <Parameter name="Value" type="double" value="0.35"/>
      </ParameterList>
      <ParameterList name="Intrinsic Permeability: Uniform">
        <Parameter name="Value" type="double" value="1.0e-10"/>
      </ParameterList>

      <ParameterList name="Dispersion Tensor: Uniform Isotropic">
        <Parameter name="alphaL" type="double" value="21.3"/>
        <Parameter name="alphaT" type="double" value="4.3"/>
      </ParameterList>
      <ParameterList name="Molecular Diffusion: Uniform">
        <Parameter name="Value" type="double" value="0.0"/>
      </ParameterList>
      <ParameterList name="Tortuosity: Uniform">
        <Parameter name="Value" type="double" value="0.0"/>
      </ParameterList>

      <ParameterList name="Capillary Pressure: None"/>
    </ParameterList>
  </ParameterList>

  <!-- Phases: -->
  <ParameterList name="Phase Definitions">
    <ParameterList name="Aqueous">
      <ParameterList name="Phase Properties">
        <ParameterList name="Viscosity: Uniform">
          <Parameter name="Viscosity" type="double" value="1.002e-3"/>
        </ParameterList>
        <ParameterList name="Density: Uniform">
          <Parameter name="Density" type="double" value="998.2"/>
        </ParameterList>
      </ParameterList>
      <ParameterList name="Phase Components">
        <ParameterList name="Water">
          <Parameter name="Component Solutes" type="Array(string)" value="{Tc-99}"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!-- Initial Conditions: -->
  <ParameterList name="Initial Conditions">
    <ParameterList name="All">
      <Parameter name="Assigned Regions" type="Array(string)" value="{All domain}"/>

      <ParameterList name="IC: Uniform Pressure">
        <Parameter name="Phase" type="string" value="Aqueous"/>
        <Parameter name="Value" type="double" value="101325.0"/>
      </ParameterList>
      <ParameterList name="IC: Uniform Velocity">
        <Parameter name="Velocity Vector" type="Array(double)" value="{1.8634e-06,0.0}"/>
      </ParameterList>

      <ParameterList name="Solute IC">
        <ParameterList name="Aqueous">
          <ParameterList name="Water">
            <ParameterList name="Tc-99">
              <ParameterList name="IC: Uniform Concentration">
                <Parameter name="Value" type="double" value="0.0"/>
              </ParameterList>
              <Parameter name="Concentration Units" type="string" value="Molarity"/>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!-- Boundary Conditions -->
  <!--ParameterList name="Boundary Conditions" -->

  <!-- Source Terms -->
  <ParameterList name="Sources">
    <ParameterList name="SRC 1">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Well}"/>
      <ParameterList name="Source: Volume Weighted">
      </ParameterList>

      <ParameterList name="Solute SOURCE">
        <ParameterList name="Aqueous">
          <ParameterList name="Water">
            <ParameterList name="Tc-99">
              <ParameterList name="Source: Flow Weighted Concentration">
                <Parameter name="Times" type="Array(double)" value="{0.0, 1.0E7}"/>
                <Parameter name="Time Functions" type="Array(string)" value="{Constant}"/>
                <Parameter name="Values" type="Array(double)" value="{8.1483e-08, 8.1483e-08}"/>
              </ParameterList>
              <Parameter name="Concentration Units" type="string" value="Molar Concentration"/>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Output">
    <ParameterList name="Time Macros">
      <ParameterList name="Observation Times">
        <Parameter name="Values" type="Array(double)" value="{1.2096E+08}"/>
      </ParameterList>

      <ParameterList name="EveryYear">
        <Parameter name="Start_Period_Stop" type="Array(double)" value="{0, 1.0e+7, 1.0e+9}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Cycle Macros">
      <ParameterList name="EveryCycle">
        <Parameter name="Start_Period" type="Array(int)" value="{0, 1}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Visualization Data">
      <Parameter name="File Name Base" type="string" value="plot"/>
      <Parameter name="File Name Digits" type="string" value="5"/>
      <Parameter name="Time Macro" type="Array(string)" value="{EveryYear}"/>
      <!--Parameter name="Cycle Macro" type="string" value="EveryCycle"/-->
    </ParameterList>

    <ParameterList name="Observation Data">
      <Parameter name="Observation Output Filename" type="string" value="obs5_2_1.out"/>
      <ParameterList name="Point 01">
        <Parameter name="Region" type="string" value="Dnwind150"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 02">
        <Parameter name="Region" type="string" value="Dnwind090"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 03">
        <Parameter name="Region" type="string" value="Dnwind030"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>

      <ParameterList name="Point 04">
        <Parameter name="Region" type="string" value="Upwind030"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 05">
        <Parameter name="Region" type="string" value="Upwind090"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 06">
        <Parameter name="Region" type="string" value="Upwind150"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 07">
        <Parameter name="Region" type="string" value="Upwind210"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 08">
        <Parameter name="Region" type="string" value="Upwind270"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 09">
        <Parameter name="Region" type="string" value="Upwind330"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 10">
        <Parameter name="Region" type="string" value="Upwind390"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
      <ParameterList name="Point 11">
        <Parameter name="Region" type="string" value="Upwind450"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Tc-99 Aqueous concentration"/>
        <Parameter name="Time Macro" type="string" value="Observation Times"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>
</ParameterList>
