@article{PAForsyth_YSWu_KPruess_1995,
  title    = "Robust numerical methods for saturated-unsaturated flow 
              with dry initial conditions in heterogeneous media",
  journal  = AdvancesINWaterResources,
  volume   = 18,
  number   = 1,
  pages    = {25--38},
  year     = 1995,
  doi      = {10.1016/0309-1708(95)00020-J},
  author   = {P. A. Forsyth and Y. S. Wu and K. Pruess},
  abstract = {A robust numerical method for saturated-unsaturated flow
              is developed which uses a monotone discretization and
              variable substitution. This method is compared to a
              conventional formulation and to a two phase (active air
              phase) model. On some published test examples of
              infiltration into dry media, the variable substitution
              method shows an order of magnitude improvement (in terms
              of nonlinear iterations) compared to the conventional
              pressure based method. One, two and three dimensional
              computations using both finite element and finite volume
              discretizations are presented.}
}

