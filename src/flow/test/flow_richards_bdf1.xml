<ParameterList name="Main">
  <Parameter name="Native Unstructured Input" type="bool" value="true"/>
  <Parameter name="grid_option" type="string" value="Unstructured"/>

  <ParameterList name="Mesh">
    <Parameter name="Framework" type="string" value="mstk::mesh"/>
  </ParameterList>    
   
  <!--  REGIONS: this super input desk will be used to cut-off smaller 
  input decks for other flow tests -->
  <ParameterList name="Regions">
    <ParameterList name="Computational domain">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array double" value="{0.0,0.0,-10.0}"/>
        <Parameter name="High Coordinate" type="Array double" value="{1.0,1.0,0.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Material 1 Region">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array double" value="{0.0,0.0,-10.0}"/>
        <Parameter name="High Coordinate" type="Array double" value="{1.0,1.0,-5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Material 2 Region">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array double" value="{0.0,0.0,-5.0}"/>
        <Parameter name="High Coordinate" type="Array double" value="{1.0,1.0,0.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Left side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{0.0,0.5,-5.0}"/>
        <Parameter name="Direction" type="Array double" value="{-1.0,0.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Right side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{1.0,0.5,-5.0}"/>
        <Parameter name="Direction" type="Array double" value="{1.0,0.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Front side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{0.5,0.0,-5.0}"/>
        <Parameter name="Direction" type="Array double" value="{0.0,-1.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Back side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{0.5,1.0,-5.0}"/>
        <Parameter name="Direction" type="Array double" value="{0.0,1.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Bottom side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{0.5,0.5,-10.0}"/>
        <Parameter name="Direction" type="Array double" value="{0.0,0.0,-1.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Top side">
      <ParameterList name="Region: Plane">
        <Parameter name="Location" type="Array double" value="{0.5,0.5,0.0}"/>
        <Parameter name="Direction" type="Array double" value="{0.0,0.0,1.0}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--  FLOW: this super input desk will be used to cut-off smaller 
  input deks for other flow tests -->
  <ParameterList name="Flow">
    <ParameterList name="Richards Problem">
      <Parameter name="Atmospheric pressure" type="double" value="1"/>

      <ParameterList name="Diffusion Preconditioner">
        <ParameterList name="ML Parameters">
          <Parameter name="ML output" type="int" value="0"/>
          <Parameter name="aggregation: damping factor" type="double" value="1.33"/>
          <Parameter name="aggregation: nodes per aggregate" type="int" value="3"/>
          <Parameter name="aggregation: threshold" type="double" value="0"/>
          <Parameter name="aggregation: type" type="string" value="Uncoupled"/>
          <Parameter name="coarse: max size" type="int" value="128"/>
          <Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
          <Parameter name="cycle applications" type="int" value="2"/>
          <Parameter name="eigen-analysis: iterations" type="int" value="10"/>
          <Parameter name="eigen-analysis: type" type="string" value="cg"/>
          <Parameter name="max levels" type="int" value="40"/>
          <Parameter name="prec type" type="string" value="MGW"/>
          <Parameter name="smoother: damping factor" type="double" value="1"/>
          <Parameter name="smoother: pre or post" type="string" value="both"/>
          <Parameter name="smoother: sweeps" type="int" value="2"/>
          <Parameter name="smoother: type" type="string" value="Gauss-Seidel"/>
        </ParameterList>
      </ParameterList>
      <ParameterList name="Richards model evaluator">
        <Parameter name="Absolute error tolerance" type="double" value="0.1"/>
        <Parameter name="Relative error tolerance" type="double" value="1e-05"/>
         <ParameterList name="VerboseObject">
           <Parameter name="Verbosity Level" type="string" value="high"/>
         </ParameterList>
      </ParameterList>

      <ParameterList name="steady time integrator">
         <ParameterList name="VerboseObject">
           <Parameter name="Verbosity Level" type="string" value="high"/>
         </ParameterList>
         <Parameter name="error abs tol" type="double" value="1"/>
         <Parameter name="error rel tol" type="double" value="0"/>
         <Parameter name="limit iterations" type="int" value="20"/>
         <Parameter name="max iterations" type="int" value="15"/>
         <Parameter name="max preconditioner lag iterations" type="int" value="20"/>
         <Parameter name="max time step" type="double" value="6e+10"/>
         <Parameter name="min iterations" type="int" value="10"/>
         <Parameter name="nonlinear tolerance" type="double" value="0.001"/>
         <Parameter name="time step increase factor" type="double" value="1.25"/>
         <Parameter name="time step reduction factor" type="double" value="0.8"/>
      </ParameterList>

      <Parameter name="Upwind relative permeability" type="bool" value="true"/>

      <ParameterList name="Water retention models">
        <ParameterList name="Water Retention Model for Material 1 Region">
          <Parameter name="Region" type="string" value="Material 1 Region"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="1.0"/>
          <Parameter name="van Genuchten m" type="double" value="0.5"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.0"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Material 2 Region">
          <Parameter name="Region" type="string" value="Material 2 Region"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="1.0"/>
          <Parameter name="van Genuchten m" type="double" value="0.5"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.0"/>
        </ParameterList>
      </ParameterList>

      <ParameterList name="boundary conditions">
        <ParameterList name="mass flux">
          <ParameterList name="BC 0">
            <Parameter name="regions" type="Array string" value="{Left side, Right side, Front side, Back side}"/>
            <ParameterList name="outward mass flux">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="0.0"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
        <ParameterList name="pressure">
          <ParameterList name="BC 1">
            <Parameter name="regions" type="Array string" value="{Top side}"/>
            <ParameterList name="boundary pressure">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="0.0"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="BC 2">
            <Parameter name="regions" type="Array string" value="{Bottom side}"/>
            <ParameterList name="boundary pressure">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="0.0"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--  STATE: this super input desk will be used to cut-off smaller 
  input deks for other flow tests -->
  <ParameterList name="State">
    <Parameter name="Component Solutes" type="Array string" value="{Tc-99}"/>
    <Parameter name="Constant viscosity" type="double" value="1.0"/>
    <Parameter name="Constant water density" type="double" value="1.0"/>
    <Parameter name="Constant water saturation" type="double" value="1"/>
    <Parameter name="Gravity x" type="double" value="0"/>
    <Parameter name="Gravity y" type="double" value="0"/>
    <Parameter name="Gravity z" type="double" value="-1.0"/>

    <Parameter name="Number of component concentrations" type="int" value="1"/>
    <Parameter name="Number of mesh blocks" type="int" value="2"/>

    <ParameterList name="Mesh block 1">
      <Parameter name="Constant component concentration 0" type="double" value="0.0"/>
      <Parameter name="Constant vertical permeability" type="double" value="2.0"/>
      <Parameter name="Constant horizontal permeability" type="double" value="2.0"/>
      <Parameter name="Constant porosity" type="double" value="0.2"/>
      <Parameter name="Region" type="string" value="Material 1 Region"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="reference value" type="double" value="0.0"/>
      </ParameterList>
<!--
      <ParameterList name="uniform saturation">
        <Parameter name="value" type="double" value="1.0"/>
      </ParameterList>
-->
    </ParameterList>
    <ParameterList name="Mesh block 2">
      <Parameter name="Constant component concentration 0" type="double" value="0.0"/>
      <Parameter name="Constant vertical permeability" type="double" value="0.5"/>
      <Parameter name="Constant horizontal permeability" type="double" value="0.5"/>
      <Parameter name="Constant porosity" type="double" value="0.2"/>
      <Parameter name="Region" type="string" value="Material 2 Region"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="reference value" type="double" value="0.0"/>
      </ParameterList>
<!--
      <ParameterList name="uniform saturation">
        <Parameter name="value" type="double" value="1.0"/>
      </ParameterList>
-->
    </ParameterList>
  </ParameterList>
  <ParameterList name="Visualization Data">
    <ParameterList name="Cycle Data">
      <Parameter name="End" type="int" value="-1"/>
      <Parameter name="Interval" type="int" value="1"/>
      <Parameter name="Start" type="int" value="0"/>
    </ParameterList>
    <Parameter name="File Name Base" type="string" value="plot"/>
    <Parameter name="File Name Digits" type="string" value="5"/>
  </ParameterList>
</ParameterList>

