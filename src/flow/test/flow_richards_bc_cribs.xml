      </ParameterList>
    </ParameterList>
    <Parameter name="Number of component concentrations" type="int" value="1"/>
    <Parameter name="Number of mesh blocks" type="int" value="8"/>
  </ParameterList>

  <!--  TRANSPORT:  -->
  <ParameterList name="Transport">
    <Parameter name="CFL" type="double" value="1"/>
    <ParameterList name="Transport BCs">
      <ParameterList name="BC 0">
        <Parameter name="Component 0" type="Array double" value="{0, 0}"/>
        <Parameter name="Regions" type="Array string" value="{Top Surface Region}"/>
        <Parameter name="Time functions" type="Array string" value="{Constant}"/>
        <Parameter name="Times" type="Array double" value="{0, 1}"/>
      </ParameterList>
      <Parameter name="number of BCs" type="int" value="1"/>
    </ParameterList>
    <Parameter name="discretization order" type="int" value="2"/>
    <Parameter name="enable internal tests" type="string" value="no"/>
  </ParameterList>
  <ParameterList name="Visualization Data">
    <ParameterList name="Cycle Data">
      <Parameter name="End" type="int" value="-1"/>
      <Parameter name="Interval" type="int" value="100"/>
      <Parameter name="Start" type="int" value="0"/>
    </ParameterList>
    <Parameter name="File Name Base" type="string" value="viz-"/>
  </ParameterList>
</ParameterList>
