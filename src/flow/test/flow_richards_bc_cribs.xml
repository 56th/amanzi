<ParameterList name="Main">
  <Parameter name="Native Unstructured Input" type="bool" value="true"/>
  <Parameter name="grid_option" type="string" value="Unstructured"/>

  <ParameterList name="Checkpoint Data">
    <ParameterList name="Cycle Data">
      <Parameter name="End" type="int" value="-1"/>
      <Parameter name="Interval" type="int" value="1000"/>
      <Parameter name="Start" type="int" value="0"/>
    </ParameterList>
    <Parameter name="File Name Base" type="string" value="dump-"/>
  </ParameterList>
  <ParameterList name="Domain">
    <Parameter name="Spatial Dimension" type="int" value="3"/>
  </ParameterList>

  <!--  FLOW:  -->
  <ParameterList name="Flow">
    <ParameterList name="Richards Problem">
      <Parameter name="Atmospheric pressure" type="double" value="101325"/>
      <ParameterList name="Diffusion Preconditioner">
        <ParameterList name="ML Parameters">
          <Parameter name="ML output" type="int" value="1"/>
          <Parameter name="aggregation: damping factor" type="double" value="1.333"/>
          <Parameter name="aggregation: nodes per aggregate" type="int" value="3"/>
          <Parameter name="aggregation: threshold" type="double" value="0.001"/>
          <Parameter name="aggregation: type" type="string" value="Uncoupled"/>
          <Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
          <Parameter name="coarse: max size" type="int" value="5"/>
          <Parameter name="coarse: sweeps" type="int" value="5"/>
          <Parameter name="coarse: damping factor" type="double" value="1.0"/>
          <Parameter name="cycle applications" type="int" value="2"/>
          <Parameter name="eigen-analysis: iterations" type="int" value="10"/>
          <Parameter name="eigen-analysis: type" type="string" value="cg"/>
          <Parameter name="max levels" type="int" value="40"/>
          <Parameter name="prec type" type="string" value="MGW"/>
          <Parameter name="smoother: damping factor" type="double" value="1.0"/>
          <Parameter name="smoother: pre or post" type="string" value="both"/>
          <Parameter name="smoother: sweeps" type="int" value="2"/>
          <Parameter name="smoother: type" type="string" value="Gauss-Seidel"/>
        </ParameterList>
      </ParameterList>

      <ParameterList name="Steady state solution">
        <Parameter name="method" type="string" value="Picard"/>
        <Parameter name="Verbosity level" type="string" value="medium"/>

        <ParameterList name="Error control">
          <Parameter name="Absolute error tolerance" type="double" value="100"/>
          <Parameter name="Relative error tolerance" type="double" value="1e-5"/>
          <Parameter name="Convergence tolerance" type="double" value="1e-12"/>
          <Parameter name="Maximal number of iterations" type="int" value="400"/>
        </ParameterList>

        <ParameterList name="Time control">
          <Parameter name="Start pseudo time" type="double" value="0.0"/>
          <Parameter name="End pseudo time" type="double" value="1e+10"/>
          <Parameter name="Initial time step" type="double" value="1e-7"/>
          <Parameter name="Maximal time step" type="double" value="1e+7"/>
        </ParameterList>

        <ParameterList name="Linear solvers"> 
          <Parameter name="Iterative method" type="string" value="CGS"/>
          <Parameter name="Error tolerance" type="double" value="1e-14"/>
          <Parameter name="Maximal number of iterations" type="int" value="100"/>
        </ParameterList>

        <ParameterList name="Nonlinear solvers">
          <Parameter name="Maximum number of BDF tries" type="int" value="10"/>
          <Parameter name="NKA drop tolerance" type="double" value="0.05"/>
          <Parameter name="NKA max vectors" type="int" value="5"/>
          <Parameter name="Nonlinear solver max iterations" type="int" value="10"/>
          <Parameter name="Nonlinear solver tolerance" type="double" value="0.01"/>
          <ParameterList name="VerboseObject">
            <Parameter name="Verbosity Level" type="string" value="medium"/>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <Parameter name="Relative permeability method" type="string" value="Upwind with gravity"/>

      <ParameterList name="Water retention models">
        <ParameterList name="Water Retention Model for Bfr">
          <Parameter name="Region" type="string" value="Bfr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="0.000194"/>
          <Parameter name="van Genuchten m" type="double" value="0.28571"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.103"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Hcsr">
          <Parameter name="Region" type="string" value="Hcsr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="0.000735"/>
          <Parameter name="van Genuchten m" type="double" value="0.51138"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.074"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Hfsr">
          <Parameter name="Region" type="string" value="Hfsr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="0.000214"/>
          <Parameter name="van Genuchten m" type="double" value="0.60112"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.089"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Hgr">
          <Parameter name="Region" type="string" value="Hgr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="0.000174"/>
          <Parameter name="van Genuchten m" type="double" value="0.42029"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.134"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for CCuzr">
          <Parameter name="Region" type="string" value="CCuzr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="5.11e-05"/>
          <Parameter name="van Genuchten m" type="double" value="0.55536"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.097"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for CCugr">
          <Parameter name="Region" type="string" value="CCugr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="0.000174"/>
          <Parameter name="van Genuchten m" type="double" value="0.42029"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.134"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Rlmr">
          <Parameter name="Region" type="string" value="Rlmr"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="5.11e-05"/>
          <Parameter name="van Genuchten m" type="double" value="0.55536"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.097"/>
        </ParameterList>
        <ParameterList name="Water Retention Model for Rwiar">
          <Parameter name="Region" type="string" value="Rwiar"/>
          <Parameter name="Water retention model" type="string" value="van Genuchten"/>
          <Parameter name="van Genuchten alpha" type="double" value="8.17e-05"/>
          <Parameter name="van Genuchten m" type="double" value="0.39759"/>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.135"/>
        </ParameterList>
      </ParameterList>

      <ParameterList name="boundary conditions">
        <ParameterList name="mass flux">
          <ParameterList name="BC 0">
            <ParameterList name="outward mass flux">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="-1.1076e-07"/>
              </ParameterList>
            </ParameterList>
            <Parameter name="regions" type="Array string" value="{Top Surface Region}"/>
          </ParameterList>
        </ParameterList>
        <ParameterList name="pressure">
          <ParameterList name="BC 1">
            <ParameterList name="boundary pressure">
              <!--
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="101325"/>
              </ParameterList>
              -->
              <ParameterList name="function-tabular">
                <Parameter name="forms" type="Array string" value="{linear}"/>
                <Parameter name="x values" type="Array double" value="{0, 1e+9}"/>
                <Parameter name="y values" type="Array double" value="{101325, 101325}"/>
              </ParameterList>
            </ParameterList>
            <Parameter name="regions" type="Array string" value="{Bottom Surface Region}"/>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--  MPC:  -->
  <ParameterList name="MPC">
    <Parameter name="End Cycle" type="int" value="-1"/>
    <Parameter name="End Time" type="double" value="1e-12"/>
    <Parameter name="Flow model" type="string" value="Richards"/>
    <Parameter name="Initial time step" type="double" value="1e-12"/>
    <Parameter name="Start Time" type="double" value="0"/>
    <ParameterList name="VerboseObject">
      <Parameter name="Verbosity Level" type="string" value="low"/>
    </ParameterList>
    <Parameter name="disable Chemistry_PK" type="string" value="yes"/>
    <Parameter name="disable Flow_PK" type="string" value="no"/>
    <Parameter name="disable Transport_PK" type="string" value="no"/>
  </ParameterList>

  <!--  MESH:  -->
  <ParameterList name="Mesh">
    <ParameterList name="Unstructured">
      <ParameterList name="Generate Mesh">
        <Parameter name="Number of Cells" type="Array int" value="{1, 1, 516}"/>
        <Parameter name="Domain Low Corner" type="Array double" value="{0.0, 0.0, 0.0}" />
        <Parameter name="Domain High Corner" type="Array double" value="{64.5, 1.0, 103.2}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--  REGIONS:  -->
  <ParameterList name="Regions">
    <ParameterList name="90 Meter Plane Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{0, 0, 1}"/>
        <Parameter name="Location" type="Array double" value="{0, 0, 90}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Bfr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 103.2}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 96.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Hcsr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 96.0}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 82.2}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Hfsr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 82.2}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 23.4}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Hgr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 23.4}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 22.2}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="CCuzr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 22.2}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 18.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="CCugr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 18.0}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 11.4}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Rlmr">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 11.4}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 6.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Rwiar">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 6.0}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Bottom Surface Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{0, 0, -1}"/>
        <Parameter name="Location" type="Array double" value="{0, 0, 0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="East Surface Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{1, 0, 0}"/>
        <Parameter name="Location" type="Array double" value="{64.5, 0, 0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="North Surface Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{0, 1, 0}"/>
        <Parameter name="Location" type="Array double" value="{0, 1, 0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="South Surface Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{0, -1, 0}"/>
        <Parameter name="Location" type="Array double" value="{0, 0, 0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Top Surface Region">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array double" value="{64.5, 1, 103.2}"/>
        <Parameter name="Low Coordinate" type="Array double" value="{0, 0, 103.2}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="West Surface Region">
      <ParameterList name="Region: Plane">
        <Parameter name="Direction" type="Array double" value="{-1, 0, 0}"/>
        <Parameter name="Location" type="Array double" value="{0, 0, 0}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--  STATE:  -->
  <ParameterList name="State">
    <Parameter name="Component Solutes" type="Array string" value="{Tc-99}"/>
    <Parameter name="Constant viscosity" type="double" value="0.00089"/>
    <Parameter name="Constant water density" type="double" value="998"/>
    <Parameter name="Constant water saturation" type="double" value="0.5"/>
    <Parameter name="Gravity x" type="double" value="0"/>
    <Parameter name="Gravity y" type="double" value="0"/>
    <Parameter name="Gravity z" type="double" value="-9.81"/>

    <ParameterList name="Mesh block 1">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="5.436e-14"/>
      <Parameter name="Constant porosity" type="double" value="0.158"/>
      <Parameter name="Region" type="string" value="Bfr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Mesh block 2">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="4.836e-13"/>
      <Parameter name="Constant porosity" type="double" value="0.364"/>
      <Parameter name="Region" type="string" value="Hcsr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Mesh block 3">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="2.045e-13"/>
      <Parameter name="Constant porosity" type="double" value="0.388"/>
      <Parameter name="Region" type="string" value="Hfsr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Mesh block 4">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="3.00e-14"/>
      <Parameter name="Constant porosity" type="double" value="0.237"/>
      <Parameter name="Region" type="string" value="Hgr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Mesh block 5">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="5.063e-15"/>
      <Parameter name="Constant porosity" type="double" value="0.36"/>
      <Parameter name="Region" type="string" value="CCuzr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Mesh block 6">
      <Parameter name="Constant component concentration 0" type="double" value="0"/>
      <Parameter name="Constant permeability" type="double" value="3.000e-14"/>
      <Parameter name="Constant porosity" type="double" value="0.237"/>
      <Parameter name="Region" type="string" value="CCugr"/>
      <ParameterList name="linear pressure">
        <Parameter name="gradient" type="Array double" value="{0, 0, 0.0}"/>
        <Parameter name="reference coordinate" type="Array double" value="{0, 0, 0}"/>
        <Parameter name="reference value" type="double" value="101325"/>
      </ParameterList>
    </ParameterList>
