<ParameterList name="Main">
  <Parameter name="Native Unstructured Input" type="bool" value="true"/>

  <ParameterList name="Domain">
    <Parameter isUsed="true" name="Spatial Dimension" type="int" value="3"/>
  </ParameterList> 

  <ParameterList name="Mesh">
    <ParameterList name="Unstructured">
      <ParameterList name="Expert">
        <Parameter name="Framework" type="string" value="MSTK"/>
      </ParameterList>
      <ParameterList name="Generate Mesh">
        <Parameter name="Domain High Corner" type="Array(double)" value="{1.475e+02, 1.0, 1.0752e+02}"/>  
        <Parameter name="Domain Low Corner" type="Array(double)" value="{1.435e+02, 0.0, 0.0}"/>
        <Parameter name="Number of Cells" type="Array(int)" value="{4, 1, 48}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Regions">
    <ParameterList name="Bottom Surface All">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array(double)" value="{1.475e+02, 1.0, 0.0}"/>
        <Parameter name="Low Coordinate" type="Array(double)" value="{1.435e+02, 0.0, 0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Region between Plane Surface 0 and Plane Surface 1">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array(double)" value="{1.475e+02, 1.0, 5.37600e+01}"/>
        <Parameter name="Low Coordinate" type="Array(double)" value="{1.435e+02, 0.0, 0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Region between Plane Surface 1 and Plane Surface 2">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array(double)" value="{1.475e+02, 1.0, 1.0752e+02}"/>
        <Parameter name="Low Coordinate" type="Array(double)" value="{1.435e+02, 0.0, 5.376e+01}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Top Surface All">
      <ParameterList name="Region: Box">
        <Parameter name="High Coordinate" type="Array(double)" value="{1.475e+02, 1.0, 1.0752e+02}"/>
        <Parameter name="Low Coordinate" type="Array(double)" value="{1.435e+02, 0.0, 1.0752e+02}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>



  <ParameterList name="Flow">
    <ParameterList name="Richards Problem">
      <Parameter name="atmospheric pressure" type="double" value="1.01325000e+05"/>
      <Parameter name="relative permeability" type="string" value="upwind with gravity"/> 
      <Parameter name="experimental solver" type="string" value="newton"/> 

      <ParameterList name="VerboseObject">
        <Parameter name="Verbosity Level" type="string" value="high"/>
      </ParameterList>

      <ParameterList name="Water retention models">
        <ParameterList name="WRM for Region between Plane Surface 0 and Plane Surface 1">
          <Parameter name="region" type="string" value="Region between Plane Surface 0 and Plane Surface 1"/>
          <Parameter name="water retention model" type="string" value="van Genuchten"/>
          <Parameter name="regularization interval" type="double" value="0.00e+00"/>
          <Parameter name="relative permeability model" type="string" value="Mualem"/>
          <Parameter name="residual saturation" type="double" value="0.000e-00"/>
          <Parameter name="van Genuchten alpha" type="double" value="1.94670000000000010e-04"/>
          <Parameter name="van Genuchten l" type="double" value="5.00e-01"/>
          <Parameter name="van Genuchten m" type="double" value="2.29399999999999993e-01"/>
        </ParameterList>
        <ParameterList name="WRM for Region between Plane Surface 1 and Plane Surface 2">
          <Parameter name="region" type="string" value="Region between Plane Surface 1 and Plane Surface 2"/>
          <Parameter name="water retention model" type="string" value="van Genuchten"/>
          <Parameter name="regularization interval" type="double" value="0.00e+00"/>
          <Parameter name="relative permeability model" type="string" value="Mualem"/>
          <Parameter name="residual saturation" type="double" value="0.000e-00"/>
          <Parameter name="van Genuchten alpha" type="double" value="2.06740000000000010e-03"/>
          <Parameter name="van Genuchten l" type="double" value="5.00e-01"/>
          <Parameter name="van Genuchten m" type="double" value="3.00599999999999993e-01"/>
        </ParameterList>
      </ParameterList>

      <ParameterList name="boundary conditions">
        <ParameterList name="mass flux">
          <ParameterList name="BC 1">
            <ParameterList name="outward mass flux">
              <ParameterList name="function-tabular">
                <Parameter name="forms" type="Array(string)" value="{constant}"/>
                <Parameter name="x values" type="Array(double)" value="{0.00e+00, 3.15576000e+10}"/>
                <Parameter name="y values" type="Array(double)" value="{-1.10510722e-04, -1.10510722e-04}"/>
              </ParameterList>
            </ParameterList>
            <Parameter name="regions" type="Array(string)" value="{Top Surface All}"/>
          </ParameterList>
        </ParameterList>
        <ParameterList name="pressure">
          <ParameterList name="BC 0">
            <ParameterList name="boundary pressure">
              <ParameterList name="function-tabular">
                <Parameter name="forms" type="Array(string)" value="{constant}"/>
                <Parameter name="x values" type="Array(double)" value="{0.00e+00, 3.15576000e+10}"/>
                <Parameter name="y values" type="Array(double)" value="{1.01325000e+05, 1.01325000e+05}"/>
              </ParameterList>
            </ParameterList>
            <Parameter name="regions" type="Array(string)" value="{Bottom Surface All}"/>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="steady state time integrator">
        <Parameter name="linear solver" type="string" value="AztecOO"/>
        <Parameter name="preconditioner" type="string" value="Hypre AMG"/>
        <Parameter name="time integration method" type="string" value="BDF1"/>

        <ParameterList name="BDF1">
          <Parameter name="error abs tol" type="double" value="1.00e+00"/>
          <Parameter name="error rel tol" type="double" value="0.00e+00"/>
          <Parameter name="time step increase factor" type="double" value="1.25"/>
          <Parameter name="limit iterations" type="int" value="20"/>
          <Parameter name="max divergent iterations" type="int" value="3"/>
          <Parameter name="max iterations" type="int" value="15"/>
          <Parameter name="max preconditioner lag iterations" type="int" value="0"/>
          <Parameter name="max time step" type="double" value="6.00e+10"/>
          <Parameter name="min iterations" type="int" value="10"/>
          <Parameter name="nonlinear iteration damping factor" type="double" value="1.0"/>
          <Parameter name="nonlinear iteration initial guess extrapolation order" type="int" value="1"/>
          <Parameter name="nonlinear tolerance" type="double" value="1.0e-06"/>
          <Parameter name="time step reduction factor" type="double" value="0.8"/>

          <Parameter name="end time" type="double" value="3.0e+08"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>


  <ParameterList name="Preconditioners">
    <ParameterList name="Hypre AMG">
      <ParameterList name="BoomerAMG Parameters">
        <Parameter name="cycle applications" type="int" value="5"/>
        <Parameter name="smoother sweeps" type="int" value="3"/>
        <Parameter name="strong threshold" type="double" value="5.00e-01"/>
        <Parameter name="tolerance" type="double" value="1.00e-7"/>
      </ParameterList>
      <Parameter name="discretization method" type="string" value="optimized mfd"/>
    </ParameterList>
    <ParameterList name="Trilinos ML">
      <ParameterList name="ML Parameters">
        <Parameter name="ML output" type="int" value="0"/>
        <Parameter name="aggregation: damping factor" type="double" value="1.33332999999999990e+00"/>
        <Parameter name="aggregation: threshold" type="double" value="0.00e+00"/>
        <Parameter name="aggregation: type" type="string" value="Uncoupled-MIS"/>
        <Parameter name="coarse: max size" type="int" value="256"/>
        <Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
        <Parameter name="cycle applications" type="int" value="2"/>
        <Parameter name="eigen-analysis: iterations" type="int" value="10"/>
        <Parameter name="eigen-analysis: type" type="string" value="cg"/>
        <Parameter name="max levels" type="int" value="40"/>
        <Parameter name="prec type" type="string" value="MGV"/>
        <Parameter name="smoother: damping factor" type="double" value="1.00e+00"/>
        <Parameter name="smoother: pre or post" type="string" value="both"/>
        <Parameter name="smoother: sweeps" type="int" value="3"/>
        <Parameter name="smoother: type" type="string" value="Jacobi"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Solvers">
    <ParameterList name="AztecOO">
      <Parameter name="error tolerance" type="double" value="1e-14"/>
      <Parameter name="iterative method" type="string" value="GMRES"/>
      <Parameter name="maximum number of iterations" type="int" value="400"/>
      <Parameter name="preconditioner" type="string" value="Hypre AMG"/>
    </ParameterList>
  </ParameterList>

  <ParameterList name="State">
    <ParameterList name="initial conditions">
      <ParameterList name="gravity">
        <Parameter name="value" type="Array(double)" value="{0.00000000000000000e+00, 0.00000000000000000e+00, -9.80664999999999942e+00}"/>
      </ParameterList>
      <ParameterList name="fluid_viscosity">
        <Parameter name="value" type="double" value="1.00200000000000007e-03"/>
      </ParameterList>
      <ParameterList name="fluid_density">
        <Parameter name="value" type="double" value="9.98200000000000045e+02"/>
      </ParameterList>
      <ParameterList name="porosity">
        <ParameterList name="function">
          <ParameterList name="Region between Plane Surface 1 and Plane Surface 2">
            <Parameter name="region" type="string" value="Region between Plane Surface 1 and Plane Surface 2"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="2.20599999999999991e-01"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Region between Plane Surface 0 and Plane Surface 1">
            <Parameter name="region" type="string" value="Region between Plane Surface 0 and Plane Surface 1"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="2.20599999999999991e-01"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>          
        </ParameterList>
      </ParameterList>
      <ParameterList name="permeability">
        <ParameterList name="function">
          <ParameterList name="Region between Plane Surface 1 and Plane Surface 2">
            <Parameter name="region" type="string" value="Region between Plane Surface 1 and Plane Surface 2"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <Parameter name="Number of DoFs" type="int" value="3"/>
              <Parameter name="Function type" type="string" value="composite function"/>
              <ParameterList name="DoF 1 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="1.99759999999999989e-12"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="DoF 2 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="1.99759999999999989e-12"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="DoF 3 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="1.99759999999999999e-13"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Region between Plane Surface 0 and Plane Surface 1">
            <Parameter name="region" type="string" value="Region between Plane Surface 0 and Plane Surface 1"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <Parameter name="Number of DoFs" type="int" value="3"/>
              <Parameter name="Function type" type="string" value="composite function"/>
              <ParameterList name="DoF 1 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="6.93649999999999978e-11"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="DoF 2 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="6.93649999999999978e-11"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="DoF 3 Function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="6.93649999999999978e-12"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>          
        </ParameterList>
      </ParameterList>
      <ParameterList name="pressure">
        <ParameterList name="function">
          <ParameterList name="Region between Plane Surface 1 and Plane Surface 2">
            <Parameter name="region" type="string" value="Region between Plane Surface 1 and Plane Surface 2"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-linear">
                <Parameter name="y0" type="double" value="1.01325000000000000e+05"/>
                <Parameter name="x0" type="Array(double)" value="{0.00000000000000000e+00, 0.00000000000000000e+00, 0.00000000000000000e+00}"/>
                <Parameter name="gradient" type="Array(double)" value="{0.00000000000000000e+00, 0.00000000000000000e+00, -9.79351920000000064e+03}"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Region between Plane Surface 0 and Plane Surface 1">
            <Parameter name="region" type="string" value="Region between Plane Surface 0 and Plane Surface 1"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-linear">
                <Parameter name="y0" type="double" value="1.01325000000000000e+05"/>
                <Parameter name="x0" type="Array(double)" value="{0.00000000000000000e+00, 0.00000000000000000e+00, 0.00000000000000000e+00}"/>
                <Parameter name="gradient" type="Array(double)" value="{0.00000000000000000e+00, 0.00000000000000000e+00, -9.79351920000000064e+03}"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>          
        </ParameterList>
      </ParameterList>
      <ParameterList name="total_component_concentration"/>
      <ParameterList name="water_saturation">
        <ParameterList name="function">
          <ParameterList name="Region between Plane Surface 1 and Plane Surface 2">
            <Parameter name="region" type="string" value="Region between Plane Surface 1 and Plane Surface 2"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="1.00000000000000000e+00"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Region between Plane Surface 0 and Plane Surface 1">
            <Parameter name="region" type="string" value="Region between Plane Surface 0 and Plane Surface 1"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="1.00000000000000000e+00"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>          
        </ParameterList>
      </ParameterList>      
    </ParameterList>
  </ParameterList>
  <Parameter name="grid_option" type="string" value="Unstructured"/>
  <Parameter isUsed="true" name="input file name" type="string" value="test_anjac.xml"/>
</ParameterList>

