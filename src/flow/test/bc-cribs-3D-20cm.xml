  <ParameterList name="Transport">

    <Parameter name="CFL" type="double" value="1"></Parameter>
    <Parameter name="discretization order" type="int" value="1"></Parameter>
    <Parameter name="enable internal tests" type="string" value="no"></Parameter>
    <Parameter name="internal tests tolerance" type="double" value="1e-06"></Parameter>
    <Parameter name="maximal time step" type="double" value="1e+99"></Parameter>
    <Parameter name="verbosity level" type="int" value="1"></Parameter>

    <ParameterList name="Transport BCs">
      <Parameter name="number of BCs" type="int" value="1"></Parameter>
      
      <ParameterList name="BC 0">
        <Parameter name="Side set ID" type="int" value="1"></Parameter>
        <Parameter name="Type" type="string" value="Constant"></Parameter>
        <Parameter name="Component 0" type="double" value="1.0"></Parameter>
      </ParameterList>
      
    </ParameterList>
