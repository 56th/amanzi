  <ParameterList name="Flow">
    <ParameterList name="Richards Problem">
      <Parameter name="Upwind relative permeability" type="bool" value="false"></Parameter>
      <Parameter name="Atmospheric pressure" type="double" value="101325.0"></Parameter>

      <ParameterList name="Steady state solution">
        <Parameter name="method" type="string" value="BDF2"/>
        <Parameter name="Absolute error tolerance" type="double" value="1"/>
        <Parameter name="Relative error tolerance" type="double" value="1e-5"/>
        <Parameter name="Start pseudo time" type="double" value="0.0"/>
        <Parameter name="End pseudo time" type="double" value="1e+10"/>
        <Parameter name="Initial time step" type="double" value="1e-7"/>
        <Parameter name="Maximal time step" type="double" value="1e+7"/>
        <Parameter name="Convergence tolerance" type="double" value="1e-9"/>
        <Parameter name="Maximal number of iterations" type="int" value="5000"/>
        <Parameter name="Linear solver tolerance" type="double" value="1e-14"/>
        <Parameter name="Linear solver maximal iterations" type="int" value="100"/>
      </ParameterList>

      <ParameterList name="Transient solution">
        <Parameter name="method" type="string" value="BDF2"/>
        <Parameter name="Absolute error tolerance" type="double" value="1"/>
        <Parameter name="Relative error tolerance" type="double" value="1e-5"/>
        <Parameter name="Start time" type="double" value="0.0"/>
        <Parameter name="End time" type="double" value="1000.0"/>
        <Parameter name="Initial time step" type="double" value="1e-1"/>
      </ParameterList>

      <ParameterList name="Time integrator">
        <Parameter name="Nonlinear solver max iterations" type="int" value="6"></Parameter>
	<Parameter name="Nonlinear solver tolerance" type="double" value="0.01"></Parameter>
        <Parameter name="NKA max vectors" type="int" value="5"></Parameter>
	<Parameter name="NKA drop tolerance" type="double" value="5.0e-2"></Parameter>
	<Parameter name="Maximum number of BDF tries" type="int" value="10"></Parameter>
        <ParameterList name="VerboseObject">
	  <Parameter name="Verbosity Level" type="string" value="medium"></Parameter>
	</ParameterList>
      </ParameterList>	

      <ParameterList name="Water retention models">
        <ParameterList name="Backfill">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 1"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.2857"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="1.9401e-4"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.103"></Parameter> 
        </ParameterList>
        <ParameterList name="Hanford Course Sand">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 2"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.5115"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="7.3518e-4"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.074"></Parameter> 
        </ParameterList>
        <ParameterList name="Hanford Fine Sand">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 3"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.6011"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="2.1443e-4"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.089"></Parameter> 
        </ParameterList>
        <ParameterList name="Hanford Gravel">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 4"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.4203"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="1.7358e-4"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.134"></Parameter> 
        </ParameterList>
        <ParameterList name="Cold Creek Unit - caliche">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 5"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.5554"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="5.1054e-5"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.097"></Parameter> 
        </ParameterList>
        <ParameterList name="Cold Creek Unit - gravel">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 6"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.4203"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="1.7358e-4"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.134"></Parameter> 
        </ParameterList>
        <ParameterList name="Ringold Lower Mud">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 7"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.5554"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="5.1054e-5"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.097"></Parameter> 
        </ParameterList>
        <ParameterList name="Ringold - Wooded Island">
          <Parameter name="Water retention model" type="string" value="van Genuchten"></Parameter>
          <Parameter name="Region" type="string" value="layer 8"></Parameter>
          <Parameter name="van Genuchten m" type="double" value="0.3976"></Parameter>
          <Parameter name="van Genuchten alpha" type="double" value="8.1687e-5"></Parameter>
          <Parameter name="van Genuchten residual saturation" type="double" value="0.135"></Parameter> 
        </ParameterList>
      </ParameterList>

      <ParameterList name="Diffusion Preconditioner">
	<ParameterList name="ML Parameters">
	  <Parameter name="ML output" type="int" value="1"></Parameter>
  	  <Parameter name="max levels" type="int" value="40"></Parameter>
	  <Parameter name="prec type" type="string" value="MGW"></Parameter>
	  <Parameter name="cycle applications" type="int" value="2"></Parameter>
	  <Parameter name="aggregation: type" type="string" value="Uncoupled"></Parameter> 
	  <Parameter name="aggregation: damping factor" type="double" value="1.333"></Parameter>
	  <Parameter name="aggregation: threshold" type="double" value="0.0"></Parameter>
	  <Parameter name="aggregation: nodes per aggregate" type="int" value="3"></Parameter>
	  <Parameter name="eigen-analysis: type" type="string" value="cg"></Parameter>
	  <Parameter name="eigen-analysis: iterations" type="int" value="10"></Parameter>
	  <Parameter name="smoother: sweeps" type="int" value="2"></Parameter>
	  <Parameter name="smoother: damping factor" type="double" value="1.0"></Parameter>
	  <Parameter name="smoother: pre or post" type="string" value="both"></Parameter>
	  <Parameter name="smoother: type" type="string" value="Gauss-Seidel"></Parameter>
	  <Parameter name="smoother: damping factor" type="double" value="1.0"></Parameter>
	  <Parameter name="coarse: type" type="string" value="Gauss-Seidel"></Parameter>
          <Parameter name="coarse: max size" type="int" value="5"></Parameter>	   
          <Parameter name="coarse: sweeps" type="int" value="5"></Parameter>	   
          <Parameter name="coarse: damping factor" type="double" value="1.0"></Parameter>	   
	</ParameterList>
      </ParameterList>
      
      <ParameterList name="boundary conditions">
        <!-- All the pressure Dirichlet conditions go in this list except for
             static pressure head conditions which are treated specially -->
        <ParameterList name="pressure">
          <ParameterList name="bottom">
            <Parameter name="regions" type="Array string" value="{bottom side}"></Parameter>
            <ParameterList name="boundary pressure">
              <ParameterList name="function-smooth-step">
                <!-- Drop the water table from z=103.2 (top) to z=0 (bottom)
                     over the time interval [0, 1e10].  The initial pressure is
	             p0 = 1112016.18144 = 101325 + 9.81 * 998.32 * 103.2 -->
                <Parameter name="x0" type="double" value="0.0"></Parameter>
                <Parameter name="x1" type="double" value="1.0e10"></Parameter>
                <Parameter name="y0" type="double" value="1112016.18144"></Parameter>
                <Parameter name="y1" type="double" value="101325.0"></Parameter>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
        <!-- All the static head pressure conditions go in this list -->
        <ParameterList name="static head">
          <!-- This specification is equivalent to the "bottom" specification
               given in the "pressure" list and is probably preferable.
          <ParameterList name="bottom">
            <Parameter name="regions" type="Array string" value="{bottom side}" />
            <ParameterList name="water table elevation">
              <ParameterList name="function-smooth-step">
                <Parameter name="x0" type="double" value="0.0" />
                <Parameter name="x1" type="double" value="1.0e10" />
                <Parameter name="y0" type="double" value="103.2" />
                <Parameter name="y1" type="double" value="0.0" />
              </ParameterList>
            </ParameterList>
          </ParameterList> -->
        </ParameterList>
        <!-- All the mass flux conditions go in this list -->
        <ParameterList name="mass flux">
          <ParameterList name="top surface">
            <Parameter name="regions" type="Array string" value="{top side}"></Parameter>
            <ParameterList name="outward mass flux">
              <ParameterList name="function-constant">
                <!-- 3.5mm/yr = 1.109842719e-10 m/s = 0.0035/3.1536e7 -->
                <!-- mass influx = 998.32 * 1.109842719e-10 kg/s-m2   -->
                <Parameter name="value" type="double" value="-1.107978183e-7"></Parameter>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <!-- Eventually we will require explicit BC on entire boundary, but
               for now "no flow" is the default.
          <ParameterList name="vertical sides">
            <Parameter name="Region" type="Array int" value="{...}" />
            <ParameterList name="outward mass flux">
              <ParameterList name="function-constant">
                <Parameter name="value" type="double" value="0.0" />
              </ParameterList>
            </ParameterList>
          </ParameterList> -->
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>
</ParameterList>
