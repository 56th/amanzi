<ParameterList name="regions">
  <ParameterList name="Bottom Box">
    <ParameterList name="region: box">
      <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0,0.0}"/>
      <Parameter name="high coordinate" type="Array(double)" value="{1.0,1.0,0.33}"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="Bottom+Middle Box">
    <ParameterList name="region: box">
      <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0,0.0}"/>
      <Parameter name="high coordinate" type="Array(double)" value="{1.0,1.0,0.66}"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="Vertical Box">
    <ParameterList name="region: box">
      <Parameter name="low coordinate" type="Array(double)" value="{0.33,0.0,0.0}"/>
      <Parameter name="high coordinate" type="Array(double)" value="{0.66,1.0,1.0}"/>
    </ParameterList>
  </ParameterList>

  <!-- sidesets -->
  <ParameterList name="ZLO FACE Plane">
    <ParameterList name="region: plane">
      <Parameter name="point" type="Array(double)" value="{0.0,0.0,0.0}"/>
      <Parameter name="normal" type="Array(double)" value="{0.0,0.0,1.0}"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="YLO FACE Box">
    <ParameterList name="region: box">
      <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0,0.66}"/>
      <Parameter name="high coordinate" type="Array(double)" value="{1.0,0.0,1.0}"/>
    </ParameterList>
  </ParameterList>

  <!-- cell set based on point -->

  <ParameterList name="Sample Point 1">
    <ParameterList name="region: point">
      <Parameter name="coordinate" type="Array(double)" value="{0.33333333333333333,0.33333333333333333333,0.3333333333333333333}"/>
    </ParameterList>
  </ParameterList>
</ParameterList>
