# CMake CGNS configure file

# Now run the patch script
execute_process(
  COMMAND sh @CGNS_sh_configure@
    RESULT_VARIABLE result
    WORKING_DIRECTORY "@CGNS_source_dir@"
)

if (result)
  set(msg "CGNS configure command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

              
