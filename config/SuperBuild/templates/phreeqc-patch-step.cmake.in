# CMake Phreeqc patch file

# Now run the patch script
set(command sh @PHREEQC_sh_patch@)
execute_process(
  COMMAND ${command}
  WORKING_DIRECTORY "@PHREEQC_source_dir@"
  RESULT_VARIABLE result
)

if (result)
  set(msg "Phreeqc patch command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  
               
