# CMake CGNS install file

# Now run the install script
execute_process(
  COMMAND sh @CGNS_sh_install@
    RESULT_VARIABLE result
    WORKING_DIRECTORY "@CGNS_source_dir@"
)

if (result)
  set(msg "CGNS install command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

              
