# CMake ASCEMIO patch file

# Now run the patch script
set(command bash @ASCEMIO_sh_patch@)
execute_process(
  COMMAND ${command}
  WORKING_DIRECTORY "@ASCEMIO_source_dir@"
  RESULT_VARIABLE result
)

if (result)
  set(msg "ASCEMIO patch command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  
               
